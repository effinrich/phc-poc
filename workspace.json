{
  "version": 2,
  "projects": {
    "phc-portal": {
      "root": "apps/phc-portal",
      "sourceRoot": "apps/phc-portal",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/phc-portal",
            "outputPath": "dist/apps/phc-portal"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "phc-portal:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "phc-portal:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "phc-portal:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/phc-portal"],
          "options": {
            "jestConfig": "apps/phc-portal/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/phc-portal/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "phc-portal-e2e": {
      "root": "apps/phc-portal-e2e",
      "sourceRoot": "apps/phc-portal-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/phc-portal-e2e/cypress.json",
            "devServerTarget": "phc-portal:serve",
            "tsConfig": "apps/phc-portal-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "phc-portal:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/phc-portal-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "portal": "apps/portal",
    "portal-e2e": "apps/portal-e2e",
    "shared-ui-components": "libs/shared-ui-components",
    "uui": "libs/uui"
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "phc-portal"
}
